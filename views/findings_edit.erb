                      </div>
                    </div>

                    <% if !@master %>
                    <div class="form-group">
                      <label class="control-label col-md-3 col-sm-3 col-xs-12" for="assessment_type">Assessment Type</label>
                      <div class="col-md-9 col-sm-9 col-xs-12">
                        <select class="form-control" name="assessment_type">
                          <% options.assessment_types.each do |type| %>
                            <% if @finding %>
                              <% if type == @finding.type %>
                                <option selected="selected"><%=type%></option>
                              <% else %>
                              <option><%=type%></option>
                              <% end %>
                            <% end %>
                          <% end %>
                        </select>
                      </div>
                    </div>
                    <% end %>

                    <% if @master %>
                      <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="approved">Approved</label>
                        <div class="col-md-9 col-sm-9 col-xs-12">
                          <% if @finding.approved %>
                            <input checked="checked" name="approved" type="checkbox"></input>
                          <% else %>
                            <input name="approved" type="checkbox"></input>
                          <% end %>
                        </div>
                      </div>
                    <% end %>


