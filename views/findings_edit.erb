                        <select class="form-control" name="risk">
                          <% risk_types = ["None", "Low", "Medium", "High", "Critical"] %>
                          <% [0,1,2,3,4].each do |r_type| %>
                          <% if r_type == @finding.risk %>
                            <option selected="selected" value="<%= r_type %>"><%=risk_types[r_type]%></option>
                          <% else %>
                            <option value="<%= r_type %>"><%=risk_types[r_type]%></option>
                          <% end %>
                          <% end %>
                        </select>
                      </div>
                    </div>

